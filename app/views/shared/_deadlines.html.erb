<div ng-app="app">
<div ng-controller="artistsCtrl">
		<div ng-init="getUserInfo(<%= current_user.id %>)" ></div>
		<!-- {{ 1 + 1 }} -->
		<table class="table">
			<tr>
				<td class="panel-title" ng-repeat="deadline in user.deadlines" ><i class = "fa fa-arrow-circle-right"> Need to do</td>
			</tr> 
			<tr>
			<td >{{ deadline.name }}</td>
			<td>{{ deadline.description }}</td>
			<td>{{ deadline.date}}</td>
			</tr>
		</table>

		<label>Deadline:</label>
		<input ng-model="newDeadlineName">
		<label>Year</label>
		<input ng-model="newExperienceYear">
		<label>Category</label>
		<input ng-model="newExperienceCategory">
		<button ng-click="addExperience(newExperienceTitle, newExperienceYear, newExperienceCategory)">Add Show</button>
	</div>
</div>



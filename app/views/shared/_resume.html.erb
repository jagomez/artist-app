<div class="resume">
<div class="col-lg-5">
	<div>
	<ul class="unstyled">
	<li><%= current_user.first_name %> <%= current_user.last_name %></li>
	<li>website: <%= current_user.website %></li>
	<li>twitter: <%= current_user.twitter %></li>
</ul>
<% current_user.educations.each do |education| %>
	<h3>Education: <%= education.school %></h2>
	<%end%>

	<% current_user.experiences.each do | experience | %>
	<h3>Shows:</h3>
	<ul class="unstyled">
		<li><%= experience.title %>, <%= experience.year %></li>
		
		<%end%>
	</ul>
</div>

<div>
</div>
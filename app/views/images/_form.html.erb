<div class="image-container">
	<h1>Add a New Image</h1>
		<%= simple_form_for @image do |f| %>
			<%= f.label :image %>
			<%= f.input :image %>

			<%= f.label :title %>
			<%= f.text_field :title %>

			<%= f.label :medium %>
			<%= f.text_field :medium %>

			<%= f.label :year %>
			<%= f.text_field :year %>

			<%= f.label :description %>
			<%= f.text_field :description %>

			<%= f.label :size %>
			<%= f.text_field :size %>

		<%= f.button :submit %>
		<% end %>
</div>